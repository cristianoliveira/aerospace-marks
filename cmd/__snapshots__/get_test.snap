
[TestGetCommand/shows_only_the_marked_windows - 1]
Context:
  marks:
    - mark: mark1
      window_id: 1
  windows:
    - app-bundle-id: ""
      app-name: app1
      window-id: 1
      window-title: title1
      workspace: ""
    - app-bundle-id: ""
      app-name: app2
      window-id: 2
      window-title: title2
      workspace: ""
    - app-bundle-id: ""
      app-name: app3
      window-id: 3
      window-title: title3
      workspace: ""

Command:
  $ aerospace-marks get mark1

Result:
  stdout:
    1 | app1 | title1
  stderr: ""
---

